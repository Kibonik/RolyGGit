<div class="container">
  <h1><%= @quest.title %></h1>
  <p><strong>Text:</strong></p>
  <p><%= @quest.body %></p>



<%= link_to 'Back', quests_path %>

<table class="table-bordered">
  <tr>
    <th>Player</th>
    <th> </th>
  </tr>

  <% @quest.players.each do |player| %>
    <tr><td> <%= link_to player.name, player_path(player) %> </td></tr>
  <% end %>

</table>
</div>